	GD_CC_SWITCHING,
	ASK_SYSTEM_INFORMATION,
	ASK_FLOW_SETTING,
	ASK_ALARM_INFORMATION,
    EXECUTE_ZERO_CALIBATION_OR_STOPWARINGUP,
