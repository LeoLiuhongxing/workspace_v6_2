#ifndef LED_DRIVER_H_
#define LED_DRIVER_H_

#define CAT3626_ADDRESS 0xCC;
#define POWER_LIGHT 0//register A
#define OPE_LIGHT   0//register A
#define ALM_LIGHT   1//register B
#define GD_LIGHT    1//register B
#define CC_LIGHT    2//register C

#define POWER_LIGHT_ON 0//register A
#define OPE_LIGHT_ON   1//register A
#define ALM_LIGHT_ON   2//register B
#define GD_LIGHT_ON    3//register B
#define CC_LIGHT_ON    4//register C

#endif /* LED_DRIVER_H_ */
