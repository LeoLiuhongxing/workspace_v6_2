#ifndef COM_PROTOCOL_H_
#define COM_PROTOCOL_H_

enum COMMAND_8{
	//8 X
	STOP_FUNCTION,//FOR ALL 0
	GD_CC_SWITCHING,
	ASK_SYSTEM_INFORMATION,
	ASK_FLOW_SETTING, //MFC 3
	ASK_ALARM_INFORMATION,//MFC 4
    EXECUTE_ZERO_CALIBATION_OR_STOPWARINGUP,//MFC 5
};

enum COMMAND_9{
    //9 X GD
    SET_GD_PR,
	SET_NUMBER_DIV,
	GD_PURGE1_ON_OFF,
	GD_PURGE2_ON_OFF,
};

enum COMMAND_A{
	//A X
    Output_GAS_STOP_GENERATION,
	CC_OZONIZER_STATE,
    INTRODUCE_O2,
	SET_CC_OZONE_GENERATION_RATE,
};

enum COMMAND_B{
	//B X
	SOLENOID_VALUE,
    ASK_SOLENODI_VALUE_STATUS,
	SET_MFC_APPLIED_VOLTTAGE,
    ASK_MFC_APPLIED_VOLATAGE,
    ASK_ZERO_CALIBRATION_RANGE,
    SET_ZERO_SPAN_GAS_FLOW_RATE,
	ASK_ZERO_SPAN_GAS_FLOW_RATE,
};

};
void stop_function_response(void);




#endif /* COM_PROTOCOL_H_ */
